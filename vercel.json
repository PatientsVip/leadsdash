{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/sid73",
      "destination": "/sid73.html"
    },
    {
      "source": "/sid82",
      "destination": "/sid82.html"
    },
    {
      "source": "/(.*)",
      "destination": "/404.html"
    }
  ]
} 